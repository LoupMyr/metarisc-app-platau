{% extends 'layouts/index.twig' %}

{% use 'layouts/navbar.twig' %}
{% block content %}
    <br><br>
    <h1 class="text-center">Organisation</h1>
    <br><br>
    <img src="{{ organisation.getLogoUrl }}" alt="logo" title="{{ organisation.nom }}" class="rounded mx-auto d-block" width="200" height="200">
    <br>
    <div class="text-center">
        <ul style="list-style: none">
            <li class="text-primary">Nom: {{ organisation.nom }}</li>
            <li>Id: {{ organisation.id }}</li>
            <li>Type: {{ organisation.type }}</li>
            <li>Lien: {{ organisation.getLogoUrl }}</li>
            <br>
        </ul>
    </div>
{% endblock %}
